{
  "version": "0.2",
  "language": "en",
  "allowCompoundWords": true,
  "enableFiletypes": [
    "vue"
  ],
  "languageSettings": [
    {
      "languageId": "vue",
      "dictionaries": [
        "html",
        "fonts",
        "typescript",
        "javascript",
        "css"
      ]
    }
  ],
  "words": [
    "awscli",
    "browserlistrc",
    "cloudfront",
    "desr",
    "nuxt",
    "nuxtignore",
    "nuxtjs",
    "vuex",
    "waicode"
  ],
  "ignorePaths": [
    ".vscode",
    ".devcontainer",
    ".git",
    "node_modules",
    "package.json",
    "yarn.lock",
    ".history",
    "dist",
    ".cspell.json"
  ]
}
